package cajero;

import javax.swing.JOptionPane;

public class Usuario extends CajeroAutomatico{
	
	@Override
	public void Transacciones() {
		
		System.out.println("---------------------------");
		System.out.println("Tu saldo es: "+getSaldo());
		System.out.println("---------------------------");
		
		JOptionPane.showMessageDialog(null, "Tu saldo es: "+getSaldo());
		
	}

}
